<%= form_with(model: identifier, local: true, multipart: true) do |form| %>
  <% if identifier.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(identifier.errors.count, "error") %> prohibited this identifier from being saved:</h2>

      <ul>
      <% identifier.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :value, "Identitfier" %>
    <%= form.text_field :value %>
  </div>

  <div>
    <%= file_field_tag :file %>
